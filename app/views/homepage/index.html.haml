.main-container{ 'ng-controller' => 'membersController', 'ng-init' => 'init()' }
  .topbar-member
    .col-md-4
      %h4 Members
    .col-md-4
      %input.form-control{ placeholder: 'Search', type: 'text' }
    .col-md-4
      %button.btn.btn-primary.add-member-btn{ type: 'button' } Add New Member

  .members-list
    %ul{ 'ng-repeat' => 'member in members' }
      %li {{member.name}}
